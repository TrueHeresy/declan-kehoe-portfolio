{
  "siteMetadata": {
    "language": "en",
    "siteUrl": "https://gatsby-starter-portfolio-minimal-theme.netlify.app/",
    "title": "Declan Kehoe Portfolio",
    "titleTemplate": "%s Â· Declan Kehoe Portfolio",
    "description": "A portfolio for Declan Kehoe, built using the portofolio minimal Gatsby starter.",
    "author": "Declan Kehoe",
    "avatar": "../content/images/declan.jpg",
    "bio": "Declan Kehoe is a recent graduate who is looking for a job, and would like to show you some things he worked on so that you may want to hire him!",
    "social": {
      "behance": "https://behance.com",
      "github": "https://github.com/True-Heresy",
      "medium": "https://medium.com",
      "linkedin": "https://www.linkedin.com/in/declan-kehoe/",
      "mail": "mailto:kehoe.declan@icloud.com"
    }
  },
  "siteConfiguration": {
    "logo": { "text": "Declan Kehoe" },
    "navigation": {
      "header": [
        { "label": "Overview", "url": "/#about" },
        { "label": "Project Writeups", "url": "/blog" }
      ],
      "ctaButton": {
        "openNewTab": true,
        "label": "CV",
        "url": "/cv.pdf"
      },
      "footer": [
        { "label": "Privacy", "url": "/privacy" },
        { "label": "Imprint", "url": "/imprint" }
      ]
    },
    "featureToggles": {
      "useDarkModeAsDefault": false,
      "useDarkModeBasedOnUsersPreference": true,
      "useCookieBar": false
    }
  }
}
